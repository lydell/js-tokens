{
  "name": "js-tokens",
  "version": "5.0.0",
  "author": "Simon Lydell",
  "license": "MIT",
  "description": "A regex that tokenizes JavaScript.",
  "keywords": [
    "JavaScript",
    "js",
    "token",
    "tokenize",
    "regex"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": "lydell/js-tokens",
  "scripts": {
    "doctoc": "doctoc README.md",
    "eslint": "eslint . --fix",
    "eslint:check": "eslint .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "npm run eslint:check && npm run prettier:check && npm run build && jest && npm run esprima-compare",
    "esprima-compare": "node parser-compare.js esprima ./node_modules/everything.js/es2015-module.js",
    "babel-compare": "node parser-compare.js babel ./node_modules/test262-parser-tests/pass/*.js",
    "build": "coffee --compile --bare index.coffee",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "@babel/parser": "7.9.4",
    "coffeescript": "2.5.1",
    "doctoc": "1.4.0",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "23.8.2",
    "esprima": "4.0.1",
    "everything.js": "1.0.3",
    "jest": "25.2.3",
    "prettier": "2.0.2",
    "test262-parser-tests": "0.0.5"
  }
}
